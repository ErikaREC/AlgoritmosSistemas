
import java.util.Arrays;

public class NombresQuick {
	public static void main(String args[]) {
    
        String[] unsorted = {"naproxeno"," paracetamol"," rimantadina"," azitromicina"," ibuprofeno"};
        System.out.println("Unsorted array :" + Arrays.toString(unsorted));
 
        QuickSortName algorithm = new QuickSortName();
 
        algorithm.sort(unsorted);
 
     
        System.out.println("Sorted array :" + Arrays.toString(unsorted));
    }
}
 
class QuickSortName {
 
    private String input[];
    private int length;
 
    public void sort(String[] names) {
 
        if (names == null || names.length == 0) {
            return;
        }
 
        this.input = names;
        length = names.length;
        quickSort(0, length - 1);
    }
 
 
    private void quickSort(int low, int high) {
        int i = low;
        int j = high;
 
        // pivot is middle index
        String pivot = input[low + (high - low) / 2];
 
        // Divide into two arrays
        while (i <= j) {
           
            while (input[i].charAt(0) < pivot.charAt(0)) {
                i++;
            }
            while (input[j].charAt(0) > pivot.charAt(0)) {
                j--;
            }
            if (i <= j) {
                swap(i, j);
                // move index to next position on both sides
                i++;
                j--;
            }
        }
 
  
        if (low < j) {
            quickSort(low, j);
        }
 
        if (i < high) {
            quickSort(i, high);
        }
    }
 
    private void swap(int i, int j) {
        String temp = input[i];
        input[i] = input[j];
        input[j] = temp;
    }
}
